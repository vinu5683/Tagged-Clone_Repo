<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.TaggedClone" parent="Theme.Design.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/tagged_official</item>
        <item name="colorPrimaryVariant">@color/tagged_official</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/tagged_official</item>
        <item name="colorSecondaryVariant">@color/tagged_official</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.TaggedClone.NoActionBar" >
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.TaggedClone.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.TaggedClone.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>